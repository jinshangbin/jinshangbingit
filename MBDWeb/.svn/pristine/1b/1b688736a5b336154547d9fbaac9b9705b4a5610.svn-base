<script type="text/javascript">
            jQuery(document).ready(function($) {
                jQuery(".select2").select2({
                    width: '100%'
                });

                $('.table').DataTable( {
                    "columnDefs": [
                        {
                            orderable: false,

                            targets: 0 }
                    ],//第一列与第二列禁止排序
                    "order": [
                        [0, null]
                    ],//第一列排序图标改为默认
                    "scrollX": true,
                    "dom": 'rt<"bottom"<"left"i>lp<"right">f<"clear">>',
                    "searching": false,
                    "language": {
                        "sProcessing": "处理中...",
                        "sLengthMenu": "每页显示 _MENU_",
                        "sZeroRecords": "没有匹配结果",
                        "sInfo": "显示从 _START_ 到 _END_ 项结果，共 _TOTAL_ 条",
                        "sInfoEmpty": "显示第 0 至 0 项结果，共 0 项",
                        "sInfoFiltered": "(由 _MAX_ 项结果过滤)",
                        "sInfoPostFix": "",
                        "sSearch": "搜索:",
                        "sUrl": "",
                        "sEmptyTable": "表中数据为空",
                        "sLoadingRecords": "载入中...",
                        "sInfoThousands": ",",
                        "oPaginate": {
                            "sFirst": "首页",
                            "sPrevious": "上一页",
                            "sNext": "下一页",
                            "sLast": "末页"
                        }
                    },
                    "drawCallback":function(){
                        $(".paginate_button").click(function(){
                            if(!$(this).hasClass("current"))
                            {
                                var parent_val=$(".dataTables_scroll");
                                parent_val.find("input").attr("checked",false);
                                parent_val.find("span").removeClass("checed");
                            }

                        });
                    }
                } );

                $(".table tbody").on("click"," input[type=checkbox]",function(){
                    var $_currentParent=$(this).parents(".dataTables_scroll ");
                    if($_currentParent.find("tbody input:checked").length==$_currentParent.find("input").length-2){
                        $_currentParent.find("thead span").addClass("checed");
                        $_currentParent.find("thead input").prop("checked",true);
                    }
                    else
                    {
                        $_currentParent.find("thead span").removeClass("checed");
                        $_currentParent.find("thead input").prop("checked",false);
                    }
                    if($(this).prop("checked"))
                    {
                        $(this).siblings().addClass("checed");
                    }
                    else
                    {
                        $(this).siblings().removeClass("checed");
                    }
                });

                $(".table thead").on("click","input",function(){

                    if($(this).prop("checked"))
                    {
                        $(this).parents(".dataTables_scroll").find("input[type=checkbox]").prop("checked",true);
                        $(this).parents(".dataTables_scroll").find("span").addClass("checed");
                    }
                    else{
                        $(this).parents(".dataTables_scroll").find("input[type=checkbox]").prop("checked",false);
                        $(this).parents(".dataTables_scroll").find("span").removeClass("checed");
                    }
                });
                //日历插件
                jQuery('.datepicker').datetimepicker();
            });
        </script>
